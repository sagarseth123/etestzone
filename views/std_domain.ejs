                                    <% } %>
                                        <% } %>
                                            <!-- <div class="card-footer text-white" style="padding: 0px;">
                                                <h3 style="margin: 0px;padding: 10px;background-color: #72664c;">
                                                     price  /- </h3>
                                            </div> -->
                                            </button>
                                            <!-- </a> -->
                                </div>
                        </div>
                        <% } %>
                </div>
                <script>
                    function mob_payment() {
                        var x = document.getElementById('mobile_payment').style;
                        if (x.display == 'none') {
                            x.display = 'block';
                        }
                    }

                    function mp_close() {
                        var x = document.getElementById('mobile_payment').style;
                        if (x.display == 'block') {
                            x.display = 'none';
                        }
                    }
                </script>
        </div>
        </div>






        <%- include('partials/footer') %>