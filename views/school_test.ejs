<%- include('partials/header') %>
    <div class="container mt-5">
        <h1>Register</h1>

        <div class="row">
            <div class="col-sm-8">
                <div class="card">
                    <div class="card-body">

                        <!-- Makes POST request to /register route -->
                        <form action="/school_test" method="POST">
                            <div class="form-group">
                                <label for="test_name">Test ID</label>
                                <input type="text" class="form-control" name="test_name" required>
                            </div>
                            <div class="form-group">
                                <label for="Username">username</label>
                                <input type="text" class="form-control" name="username" required>
                            </div>

                            <button type="submit" class="btn btn-dark">Register</button>
                        </form>

                    </div>
                </div><br>
                <h6>You can see your result here after giving your test by giving your Usename and test id again</h6>
            </div>